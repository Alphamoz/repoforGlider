<launch>
	<!-- Package Control Actuator -->
    <node name="node_BE" pkg="control_actuator" type="node_BE.py" />
    <node name="node_MM" pkg="control_actuator" type="node_MM.py" />
    <node name="node_thruster_stern" pkg="control_actuator" type="node_thruster_stern.py" />
    <node name="node_thruster_bow" pkg="control_actuator" type="node_thruster_bow.py" />
    
    <node name="node_control_depth" pkg="control_actuator" type="node_control_depth.py" />
    <node name="node_control_pitch" pkg="control_actuator" type="node_control_pitch.py" />
    <node name="node_control_yaw" pkg="control_actuator" type="node_control_yaw.py" />
    
    <!-- Package Node GCS -->
    <node name="node_gcs" pkg="node_gcs" type="node_gcs.py" />

    <!-- Package Node Guidance -->
    <node name="node_guidance" pkg="node_guidance" type="node_guidance.py" />

    <!-- Package Node Sensor -->
    <node name="node_AltiVA500" pkg="node_sensor" type="node_AltiVA500.py" />
    <node name="node_DVL" pkg="node_sensor" type="node_DVL.py" />
    <node name="node_IMUGPS" pkg="node_sensor" type="node_IMUGPS.py" />
    <node name="node_miniCT" pkg="node_sensor" type="node_miniCT.py" />
    <node name="node_navigasi" pkg="node_sensor" type="node_navigasi.py" />
    
    <!-- name: node name shown in rqt, pkg: xml package name, type: file name -->
</launch>